{
  "expo": {
    "name": "MemoParkApp",
    "slug": "MemoParkApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "memoparkapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "config": {
        "googleMapsApiKey": "AIzaSyCOMRs9LVkFe36qYr0v1u6g5_DW-DSCYNA"
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.thanushka.MemoParkApp",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to save and find parking spots.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses your location to save and find parking spots even when the app is in the background.",
        "NSLocationAlwaysUsageDescription": "This app uses your location to save and find parking spots."
      }
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCOMRs9LVkFe36qYr0v1u6g5_DW-DSCYNA"
        }
      },
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.thanushka.MemoParkApp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "imageWidth": 200,
          "backgroundColor": "#ffffff"
        }
      ],
      [
      "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow MemoParkApp to use your location to save parking spots."
        }
      ],
      [
      "expo-image-picker",
        {
          "photosPermission": "Allow MemoParkApp to access your photos to save parking spot images.",
          "cameraPermission": "Allow MemoParkApp to access your camera to take parking spot photos."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e8d973e4-3cc0-481e-bc39-33906406a1b3"
      }
    }
  }
}
